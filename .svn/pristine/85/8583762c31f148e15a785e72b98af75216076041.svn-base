<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c"%>

<link href="https://mail1.daumcdn.net/mail_static/mint/dist/1.0.59.prod.1687410423994/static/css/main.2b60432b.css" rel="stylesheet">
<link rel="stylesheet" href="https://mail1.daumcdn.net/mail_static/mint/css/mint-editor-frame.css" type="text/css">
<script type="text/javascript" charset="UTF-8" src="https://t1.daumcdn.net/tiara/js/v1/tiara.min.js"></script>
<script defer="defer" src="https://mail1.daumcdn.net/mail_static/mint/dist/1.0.59.prod.1687410423994/static/js/main.851a429b.js"></script>

<div class="layer_box layer_popup" role="dialog" tabindex="-1"
	style="display: block; width: 777px; height: 652px;">
	<div class="inner_layer">
		<strong class="tit_mail">주소록</strong>
		<div class="search_wrap">
			<span class="txt_search">주소록 검색</span>
			<div class="search_address">
				<form role="search">
					<fieldset class="fld_mail">
						<legend class="screen_out">주소록 검색폼</legend>
						<span class="img_mail_v1 img_round"></span><span
							class="box_nameaddr"><label for="addrSearch"
							class="lab_search">이름 또는 이메일 주소로 검색</label><input type="text"
							class="tf_keyword" id="addrSearch" title="검색어 입력"
							autocomplete="off" spellcheck="false" value=""></span>
						<button type="submit" class="btn_search"
							data-tiara-action-name="주소록검색">
							<span class="img_mail_v1 ico_search">검색</span>
						</button>
					</fieldset>
				</form>
			</div>
		</div>
		<div class="mail_address">
			<div class="view_cate">
				<strong class="screen_out">주소록 분류 선택</strong>
				<ul class="tab_cate" role="tablist">
					<li class="on"><a href="#" role="tab" class="link_cate"
						aria-controls="panelPersonal" aria-selected="true">개인</a></li>
					<li class=""><a href="#" role="tab" class="link_cate"
						aria-controls="panelPersonal" aria-selected="false">그룹</a></li>
				</ul>
				<div class="box_cate">
					<strong class="screen_out">주소록 가나다순 찾기</strong>
					<div class="addr_person panel" id="panelPersonal"
						aria-labelledby="addressPersonal" tabindex="-1"
						style="overflow: auto; display: block;">
						<div class="content_wrap" style="height: 400px;">
							<strong class="screen_out">개인 메일주소 목록</strong>
							<div class="container">
								<ul class="list_mailaddr" style="overflow: auto; padding: 0px;">
									<li class="row selectable" data-key="0"
										style="top: 1px; height: 25px;"><a href="#"
										class="link_mailaddr">hachkorea &lt;hachkorea@hach.com&gt;</a></li>
								</ul>
							</div>
						</div>
					</div>
				</div>
				<button type="button" class="btn_addr btn_add add_type1"
					data-tiara-action-name="주소록_받는사람선택">
					<span class="img_mail_v1 ico_add">받는 사람으로 추가</span>
				</button>
				<button type="button" class="btn_addr btn_add add_type2"
					data-tiara-action-name="주소록_참조선택">
					<span class="img_mail_v1 ico_add">참조로 추가</span>
				</button>
				<button type="button" class="btn_addr btn_add add_type3"
					data-tiara-action-name="주소록_숨은참조선택">
					<span class="img_mail_v1 ico_add">숨은참조로 추가</span>
				</button>
			</div>
			<a href="#" class="link_admin" data-tiara-action-name="주소록관리">주소록
				관리<span class="img_mail_v1 ico_arrow"></span>
			</a>
			<div class="view_address">
				<strong class="tit_addr">받는사람 </strong>
				<div class="box_addr to_addr">
					<div class="container">
						<ul class="list_mailaddr"></ul>
					</div>
				</div>
				<button type="button" class="btn_addr delete_type1"
					data-tiara-action-name="주소록_받는사람취소">
					<span class="img_mail_v1 ico_delete">받는사람 선택 삭제</span>
				</button>
				<strong class="tit_addr">참조 </strong>
				<div class="box_addr cc_addr">
					<div class="container">
						<ul class="list_mailaddr"></ul>
					</div>
				</div>
				<button type="button" class="btn_addr delete_type2"
					data-tiara-action-name="주소록_참조취소">
					<span class="img_mail_v1 ico_delete">참조 선택 삭제</span>
				</button>
				<strong class="tit_addr">숨은참조 </strong>
				<div class="box_addr bcc_addr">
					<div class="container">
						<ul class="list_mailaddr"></ul>
					</div>
				</div>
				<button type="button" class="btn_addr delete_type3"
					data-tiara-action-name="주소록_숨은참조취소">
					<span class="img_mail_v1 ico_delete">숨은참조 선택 삭제</span>
				</button>
			</div>
		</div>
		<div class="btn_box">
			<span class="inner_btn"><button type="button"
					class="btn_g btn_check" data-tiara-action-name="주소록_저장">
					<span class="txt">저장</span>
				</button>
				<button type="button" class="btn_g" data-tiara-action-name="주소록_취소">
					<span class="txt">취소</span>
				</button></span>
		</div>
		<a href="#" class="link_close"><span class="img_mail_v1 ico_close">닫기</span></a>
	</div>
</div>

